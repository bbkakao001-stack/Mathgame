    <script>
        const answers = {
            q1: 'a', q2: 'b', q3: 'b', q4: 'b', q5: 'a',
            q6: 'b', q7: 'a', q8: 'a', q9: 'a', q10: 'a',
            q11: 'a', q12: 'a', q13: 'a', q14: 'a', q15: 'a',
            q16: 'a', q17: 'a', q18: 'a', q19: 'a', q20: 'a'
        };

        document.getElementById('quizForm').addEventListener('submit', function(e) {
            e.preventDefault();
            
            let score = 0;
            let total = 20;
            
            for (let q in answers) {
                const selected = document.querySelector(`input[name="${q}"]:checked`);
                if (selected && selected.value === answers[q]) {
                    score++;
                }
            }
            
            let emoji = '';
            let message = '';
            let percentage = (score / total) * 100;
            
            if (percentage >= 90) {
                emoji = 'üåüüéâüèÜ';
                message = '‡πÄ‡∏Å‡πà‡∏á‡∏°‡∏≤‡∏Å! ‡∏Ñ‡∏∏‡∏ì‡πÄ‡∏õ‡πá‡∏ô‡∏î‡∏≤‡∏ß‡πÄ‡∏î‡πà‡∏ô‡πÄ‡∏•‡∏¢‡∏Ñ‡πà‡∏∞!';
            } else if (percentage >= 80) {
                emoji = 'üòäüå∏‚ú®';
                message = '‡πÄ‡∏¢‡∏µ‡πà‡∏¢‡∏°‡∏°‡∏≤‡∏Å! ‡∏ó‡∏≥‡πÑ‡∏î‡πâ‡∏î‡∏µ‡∏°‡∏≤‡∏Å‡πÄ‡∏•‡∏¢‡∏Ñ‡πà‡∏∞!';
            } else if (percentage >= 70) {
                emoji = 'üëçüíêüåº';
                message = '‡∏î‡∏µ‡∏°‡∏≤‡∏Å‡πÄ‡∏•‡∏¢‡∏Ñ‡πà‡∏∞! ‡∏û‡∏±‡∏í‡∏ô‡∏≤‡∏ï‡πà‡∏≠‡πÑ‡∏õ‡∏ô‡∏∞!';
            } else if (percentage >= 60) {
                emoji = 'üòäüå∫';
                message = '‡∏ú‡πà‡∏≤‡∏ô‡πÅ‡∏•‡πâ‡∏ß‡∏Ñ‡πà‡∏∞ ‡πÄ‡∏Å‡πà‡∏á‡∏°‡∏≤‡∏Å!';
            } else {
                emoji = 'üí™üåª';
                message = '‡∏™‡∏π‡πâ‡∏ï‡πà‡∏≠‡πÑ‡∏õ‡∏ô‡∏∞‡∏Ñ‡∏∞ ‡∏Ñ‡∏∏‡∏ì‡∏ó‡∏≥‡πÑ‡∏î‡πâ!';
            }
            
            const resultDiv = document.getElementById('result');
            resultDiv.innerHTML = `
                <div class="emoji">${emoji}</div>
                <div>‡∏Ñ‡∏∞‡πÅ‡∏ô‡∏ô‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì: ${score} / ${total}</div>
                <div style="font-size: 1.2em; margin-top: 10px;">${message}</div>
                <div style="font-size: 0.9em; margin-top: 15px; opacity: 0.9;">
                    ${percentage.toFixed(0)}% üéØ
                </div>
            `;
            resultDiv.style.display = 'block';
            
            window.scrollTo({
                top: document.body.scrollHeight,
                behavior: 'smooth'
            });
        });
